{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "@typescript-eslint",
    "react",
    "only-warn"
  ],
  "globals": {
    "fetch": false
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": [
      0
    ],
    "@typescript-eslint/semi": 0,
    "semi": [
      2,
      "never"
    ],
    "import/prefer-default-export": 0,
    "import/no-cycle": [
      2,
      {
        "maxDepth": 2
      }
    ],
    "no-console": 0
  }
}